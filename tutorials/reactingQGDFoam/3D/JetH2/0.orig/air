FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      He;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
     inlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    outlet
    {
        type            zeroGradient;
    }

    "(topAndBottom|back)"
    {
        type            zeroGradient;
    }

    sym
    {
        type            symmetry; //zeroGradient;
    }

    leftwall 
    {
        type            zeroGradient;
    }
}


