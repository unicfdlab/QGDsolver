3
(
    movingWall
    {
        type            wall;
        inGroups        1(wall);
        nFaces          10;
        startFace       180;
    }
    fixedWalls
    {
        type            wall;
        inGroups        1(wall);
        nFaces          30;
        startFace       190;
    }
    frontAndBack
    {
        type            empty;
        inGroups        1(empty);
        nFaces          200;
        startFace       220;
    }
)

